{
  "enabledPlugins": {
    "ralph-loop@claude-plugins-official": true,
    "typescript-lsp@claude-plugins-official": true
  },
  "env": {
    "CLAUDE_CODE_EXPERIMENTAL_AGENT_TEAMS": "1"
  },
  "plansDirectory": "plans",
  "permissions": {
    "defaultMode": "dontAsk",
    "allow": [
      "Read",
      "Edit",
      "Write",
      "Grep",
      "Glob",
      "WebSearch",
      "Bash(npx vitest:*)",
      "Bash(npm test:*)",
      "Bash(npm run:*)",
      "Bash(git status:*)",
      "Bash(git diff:*)",
      "Bash(git init:*)",
      "Bash(git branch:*)",
      "Bash(git checkout:*)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(git push:*)",
      "Bash(git config:*)",
      "Bash(git log:*)",
      "Bash(gh pr create:*)",
      "Bash(gh pr view:*)",
      "Bash(gh pr list:*)",
      "Bash(gh pr checks:*)",
      "Bash(gh run view:*)",
      "Bash(gh api repos/*:*)",
      "Bash(npm run lint:*)",
      "Bash(xxd:*)",
      "Bash(description:run tests)",
      "Bash(description:run vitest)",
      "Bash(description:run test suite)",
      "Bash(description:run unit tests)",
      "Bash(description:run build)",
      "Bash(description:build project)",
      "Bash(description:build Next.js)",
      "Bash(description:install dependencies)",
      "Bash(description:check git status)",
      "Bash(description:show git diff)",
      "Bash(description:run development server)",
      "Bash(description:check build output)",
      "WebFetch(domain:www.sbprojects.net)",
      "WebFetch(domain:www.applefritter.com)",
      "WebFetch(domain:github.com)",
      "WebFetch(domain:raw.githubusercontent.com)",
      "WebFetch(domain:jeffavery.ca)",
      "WebFetch(domain:mdfs.net)",
      "WebFetch(domain:www.seasip.info)",
      "WebFetch(domain:floooh.github.io)",
      "WebFetch(domain:www.trs-80.com)",
      "WebFetch(domain:www.computerarcheology.com)",
      "WebFetch(domain:cpmarchives.classiccmp.org)",
      "WebFetch(domain:www.manualslib.com)",
      "WebFetch(domain:en.wikipedia.org)",
      "WebFetch(domain:misterfpga.org)",
      "WebFetch(domain:www.alnwlsn.com)",
      "WebFetch(domain:www.eevblog.com)",
      "WebFetch(domain:ui.jln.dev)",
      "WebFetch(domain:commons.wikimedia.org)",
      "mcp__browsermcp__browser_screenshot",
      "mcp__browsermcp__browser_snapshot",
      "mcp__browsermcp__browser_navigate",
      "mcp__browsermcp__browser_click",
      "mcp__browsermcp__browser_type",
      "mcp__browsermcp__browser_hover",
      "mcp__browsermcp__browser_select_option",
      "mcp__browsermcp__browser_press_key",
      "mcp__browsermcp__browser_go_back",
      "mcp__browsermcp__browser_go_forward",
      "mcp__browsermcp__browser_wait",
      "mcp__browsermcp__browser_get_console_logs"
    ]
  },
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "bun x gh-setup-hooks",
            "timeout": 120
          }
        ]
      }
    ]
  }
}
